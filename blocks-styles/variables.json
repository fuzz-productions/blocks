{
  "sans-serif-font-family" : "Graphik Web",
  "normal-weight" : "400",
  "medium-weight" : "500",
  "bold-weight" : "700",


  "interactive-hex" : "#009f6f",
  "interactive-hover-hex" : "#008b61",
  "interactive-shadow-hex" : "rgba(0, 72, 51, 0.25)",
  "interactive-shadow-hover-hex" : "rgba(0, 72, 51, 0.35)",

  "brand-01-hex" : "#00b07d",

  "ui-00-hex" : "#ffffff",
  "ui-01-hex" : "#f6f8f8",
  "ui-02-hex" : "#f5faf8",
  "ui-03-hex" : "#edf0ee",
  "ui-04-hex" : "#d9e1e0",
  "ui-05-hex" : "#bdc7c3",
  "ui-06-hex" : "#aab7b2",
  "ui-07-hex" : "#91a49e",

  "text-01-hex" : "#2b3836",
  "text-02-hex" : "#7c8c88",

  "success-hex" : "#39cf94",
  "information-hex" : "#6f6bf8",
  "warning-hex" : "#fdb100",
  "error-hex" : "#ec4f07",
  "selection-hex" : "#49a1ff",


  "interactive" : "var(--interactive, $interactive-hex)",
  "interactive-hover" : "var(--interactive-hover, $interactive-hover-hex)",
  "interactive-shadow" : "var(--interactive-shadow, $interactive-shadow-hex)",
  "interactive-shadow-hover" : "var(--interactive-shadow-hover, $interactive-shadow-hover-hex)",

  "brand" : {
    "01" : "var(--brand-01, $brand-01-hex)"
  },

  "ui" : {
    "00" : "var(--ui-00, $ui-00-hex)",
    "01" : "var(--ui-01, $ui-01-hex)",
    "02" : "var(--ui-02, $ui-02-hex)",
    "03" : "var(--ui-03, $ui-03-hex)",
    "04" : "var(--ui-04, $ui-04-hex)",
    "05" : "var(--ui-05, $ui-05-hex)",
    "06" : "var(--ui-06, $ui-06-hex)",
    "07" : "var(--ui-07, $ui-07-hex)"
  },

  "text" : {
    "01" : "var(--text-01, $text-01-hex)",
    "02" : "var(--text-02, $text-02-hex)"
  },

  "success" : "var(--success-hex, $success-hex)",
  "information" : "var(--information, $information-hex)",
  "warning" : "var(--warning, $warning-hex)",
  "error" : "var(--error, $error-hex)",
  "selection" : "var(--selection, $selection-hex)",





  "base-font-size" : "16px",

  "base-border-radius" : "7px",





  "button-height" : "44px",

  "button-font-size" : "16px",

  "button-shadow-color" : "$interactive-shadow",
  "button-hover-shadow-color" : "$interactive-shadow-hover",
  "button-focus-shadow-color" : "$interactive-shadow-hover",

  "primary-button-background-color" : "$interactive",
  "primary-button-hover-background-color" : "$interactive-hover",
  "primary-button-focus-background-color" : "$interactive-hover",

  "primary-button-border-color" : "$interactive",
  "primary-button-hover-border-color" : "$interactive-hover",
  "primary-button-focus-border-color" : "$selection",

  "primary-button-text-color" : "$ui-00",
  "primary-button-hover-text-color" : "$ui-00",
  "primary-button-focus-text-color" : "$ui-00",

  "secondary-button-background-color" : "$ui-00",
  "secondary-button-hover-background-color" : "$ui-02",
  "secondary-button-focus-background-color" : "$ui-02",

  "secondary-button-border-color" : "$ui-00",
  "secondary-button-hover-border-color" : "$ui-02",
  "secondary-button-focus-border-color" : "$selection",

  "secondary-button-text-color" : "$interactive",
  "secondary-button-hover-text-color" : "$interactive",
  "secondary-button-focus-text-color" : "$interactive",





  "checkbox-size" : "18px",

  "checkbox-text-color" : "$text-01",
  "checkbox-hover-shadow-color" : "$interactive-shadow-hover",

  "checkbox-unselected-border-color" : "$ui-04",
  "checkbox-unselected-hover-border-color" : "$ui-04",
  "checkbox-unselected-focus-border-color" : "$success",

  "checkbox-unselected-background-color" : "$ui-00",
  "checkbox-unselected-hover-background-color" : "$ui-00",
  "checkbox-unselected-focus-background-color" : "$ui-00",

  "checkbox-selected-border-color" : "$brand-01",
  "checkbox-selected-hover-border-color" : "$interactive",
  "checkbox-selected-focus-border-color" : "$success",

  "checkbox-selected-background-color" : "$brand-01",
  "checkbox-selected-hover-background-color" : "$interactive",
  "checkbox-selected-focus-background-color" : "$interactive",




  "radio-size" : "18px",

  "radio-text-color" : "$text-01",
  "radio-hover-shadow-color" : "$interactive-shadow-hover",

  "radio-unselected-border-color" : "$ui-04",
  "radio-unselected-hover-border-color" : "$ui-04",
  "radio-unselected-focus-border-color" : "$success",

  "radio-unselected-background-color" : "$ui-00",
  "radio-unselected-hover-background-color" : "$ui-00",
  "radio-unselected-focus-background-color" : "$ui-00",

  "radio-selected-border-color" : "$brand-01",
  "radio-selected-hover-border-color" : "$interactive",
  "radio-selected-focus-border-color" : "$interactive",

  "radio-selected-background-color" : "$brand-01",
  "radio-selected-hover-background-color" : "$interactive",
  "radio-selected-focus-background-color" : "$interactive"

}